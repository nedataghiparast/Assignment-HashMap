<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<DialogPane fx:controller="com.neda.LibraryApp.DialogController"
            xmlns:fx="http://javafx.com/fxml" >
    <content>
        <GridPane vgap="10" hgap="10">
            <Label GridPane.rowIndex="0" GridPane.columnIndex="0" text="Title:"/>
             <TextField GridPane.rowIndex="0" GridPane.columnIndex="1" fx:id="newBookTitleTextField"/>
            <Label GridPane.rowIndex="1" GridPane.columnIndex="0" text="Author:"/>
            <TextField GridPane.rowIndex="1" GridPane.columnIndex="1" fx:id="newBookAuthorTextField"/>
        </GridPane>
    </content>
</DialogPane>
